def maior_numero(n1: float, n2:float, n3:float):
    lista_numero = []
    lista_numero.append(n1)
    lista_numero.append(n2)
    lista_numero.append(n3)
    return max(lista_numero)


n1 = float(input('n1: '))
n2 = float(input('n2: '))
n3 = float(input('n3: '))


print(f' O maior nÃºmero Ã© {maior_numero(n1, n2, n3):.2f}')

